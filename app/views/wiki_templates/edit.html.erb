<h2><%= @wiki_template.name %></h2>

<% form = labelled_form_for @wiki_template, :url => {:action => 'update', :project_id => @project.id}, :html => {:multipart => false, :id => 'wiki_template_form'} do |f| %>
<%= render :partial => 'form', :locals => {:f => f} %>
<%= submit_tag l(:button_save) %>
 | <%= link_to l(:button_cancel), back_url if back_url.present? %>
<% end %>

<%= form if Rails::VERSION::MAJOR >= 3 %>
